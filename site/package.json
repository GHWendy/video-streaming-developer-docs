{
  "name": "example",
  "private": true,
  "version": "1.20.0",
  "license": "Apache 2.0",
  "scripts": {
    "build:clean": "yarn clean && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "build:prefix": "gatsby build --prefix-paths",
    "serve:prefix": "gatsby serve --prefix-paths",
    "test:prefix": "yarn build:prefix && yarn serve:prefix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public",
    "deploy_from_ci": "npm run build && gh-pages -m 'build: travis build and deploy' -d public -r https://$GH_TOKEN@github.ibm.com/ustream-web/developers.git",
    "build": "gatsby build --prefix-paths",
    "dev": "gatsby develop",
    "dev:clean": "gatsby clean && gatsby develop"
  },
  "dependencies": {
    "gatsby": "^2.19.0",
    "gatsby-theme-carbon": "^1.20.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0"
  }
}
